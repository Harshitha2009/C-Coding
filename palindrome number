#include<stdio.h>
int main()
{
    int n,n1,rev=0,r;
    printf("enter n value");
    scanf("%d",&rev);
    n1=n;
    while(n!=0)
    {
        r=n%10;
        rev=(rev*10)+r;
        n=(n/10);
    }
    printf("%d",rev);
    if(rev==n1)
    printf("%d is palindrome",n1);
    else
    printf("%d is not palindrome",n1);
}
